{
  "name": "container-data-receiver",
  "version": "1.0.0",
  "description": "Node.js receiver for compressed container data via MessagePack with pseudo queue processing",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "npm run test:health && npm run test:compression",
    "test:health": "curl -s http://localhost:3000/health | jq",
    "test:compression": "node test_compression.js",
    "docker:build": "docker build -t container-receiver .",
    "docker:run": "docker run -p 3000:3000 container-receiver",
    "docker:stop": "docker stop $(docker ps -q --filter ancestor=container-receiver)"
  },
  "keywords": [
    "container",
    "msgpack",
    "compression",
    "iot",
    "stress-test",
    "nodejs",
    "express"
  ],
  "author": "Container Data Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "@msgpack/msgpack": "^2.8.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourorg/container-data-receiver.git"
  },
  "bugs": {
    "url": "https://github.com/yourorg/container-data-receiver/issues"
  },
  "homepage": "https://github.com/yourorg/container-data-receiver#readme"
}
